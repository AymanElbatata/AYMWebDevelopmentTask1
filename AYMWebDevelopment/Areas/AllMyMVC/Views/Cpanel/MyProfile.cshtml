@{
    ViewBag.Title = "MyProfile";
    Layout = "~/Views/Shared/_Layout5.cshtml";
    string autocode = AYMWebDevelopment.Controllers.CurrentUserLoginData.getcurrentusrdataAuth(2);
    string email = AYMWebDevelopment.Controllers.CurrentUserLoginData.getcurrentusrdataAuth(3);
    string fullname = AYMWebDevelopment.Controllers.CurrentUserLoginData.getcurrentusrdataAuth(4);
}

    <div class="myprofile">
        @if (ViewBag.Successful != null)
        {
            <span style="color:green">
                @ViewBag.Successful
            </span>
        }
        <br />
            <img src="~/Images/smileee.png" alt="img" style="width:25%" />
    </div>


@if (User.Identity.IsAuthenticated)
{
    <h4>Welcome: @fullname &nbsp;/&nbsp; @autocode @*@HttpContext.Current.User.Identity.Name*@</h4>
    <h5>@email</h5>
    <br />
    <span>
            @Html.ActionLink("Change Password", "EditPassword", "Cpanel")
    </span>
}

@using System.Web.Optimization;
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="description" content="AYM Web Development">
    <meta name="keywords" content="AYM, Web, Development">
    <meta name="author" content="Ayman Elbatata">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewBag.Title - AYM Web_Dev</title>
    <link rel="icon" type="image/x-icon" href="@Url.Content("~/Images/logoforicon_1_JWA_icon.ico")">
    @*<link rel="icon" href="~/DesignTemps/ahmeddesign1/imges/logo/aym-webdev1.png" style="width: 300px; height: 200px">*@
    @*<link rel="icon" href="~/Images/iconPNG2.png" style="width: 300px; height: 200px">*@
    <!-- [if lt IE 9]>
    <script src="~/Scripts/AYM/html5shiv-printshiv.min.js"></script>
    <![endif]-->
    @Styles.Render("~/CSFILES1")
    @Styles.Render("~/CSFILES2")

    @Scripts.Render("~/JSFILES1")
